INCREMENT BY 1;

--Sekwencja TABELA PRACOWNICY_INFORMACJE--
 CREATE SEQUENCE pracownicy_informacje_sq
    minvalue 0
    start with 0
    increment by 1
    nocycle
    nocache
    ;
